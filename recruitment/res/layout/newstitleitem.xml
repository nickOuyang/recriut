<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/poistionsubscribeitemlay"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	android:padding="10dip" >

	<TextView
		android:id="@+id/title"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:ellipsize="end"
		android:gravity="center_vertical|left"
		android:maxLines="1"
		android:textColor="@color/blue"
		android:textSize="@dimen/subscribejobnamesize" />

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal" >

		<TextView
			android:id="@+id/fname"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:ellipsize="end"
			android:gravity="center_vertical|left"
			android:maxLines="1"
			android:maxWidth="280dip"
			android:text="sdfsdfsdfsfdsfffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff"
			android:textColor="@color/lightblack" 
			android:layout_marginLeft="@dimen/textblankpadding_large"
			/>

		<TextView
			android:id="@+id/times"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="5dip"
			android:background="@drawable/orangesmallback"
			android:gravity="center"
			android:paddingLeft="3dip"
			android:paddingRight="5dip"
			android:textColor="@color/whitecolor" />
	</LinearLayout>

</LinearLayout>